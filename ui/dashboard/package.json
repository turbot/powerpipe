    "autoprefixer": "10.4.20",
    "buffer": "6.0.3",
    "circular-dependency-plugin": "5.2.2",
    "crypto-browserify": "3.12.1",
    "fs-extra": "11.2.0",
    "if-node-version": "1.1.1",
    "lint-staged": "15.2.10",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "process": "0.11.10",
    "prop-types": "15.8.1",
    "react-scripts": "5.0.1",
    "source-map-explorer": "2.5.3",
    "storybook": "8.4.5",
    "storybook-addon-react-router-v6": "2.0.15",
    "storybook-dark-mode": "4.0.2",
