{
  "name": "powerpipe-dashboard-ui",
  "version": "0.14.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "setup-material-symbols": "node scripts/setupMaterialSymbols.js",
    "start": "if-node-version '>= 17' && craco --openssl-legacy-provider start || craco start",
    "build": "if-node-version '>= 17' && GENERATE_SOURCEMAP=false craco --openssl-legacy-provider build || GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "prettify": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "yarn run setup-material-symbols && NODE_OPTIONS=--openssl-legacy-provider storybook dev -p 6006",
    "build-storybook": "storybook build -s public"
  },
  "dependencies": {
    "@headlessui/react": "1.7.19",
    "@heroicons/react": "2.2.0",
    "@material-symbols/svg-300": "0.27.1",
    "@popperjs/core": "2.11.8",
    "@supabase/sql-formatter": "4.0.3",
    "@tanstack/react-table": "8.20.5",
    "@tanstack/react-virtual": "3.10.9",
    "color-convert": "2.0.1",
    "copy-to-clipboard": "3.3.3",
    "dagre": "0.8.5",
    "dayjs": "1.11.13",
    "deepdash": "5.3.9",
    "echarts": "5.5.1",
    "echarts-for-react": "3.0.2",
    "echarts-gl": "2.0.9",
    "file-saver": "2.0.5",
    "framer-motion": "11.11.17",
    "jq-wasm": "0.0.9",
    "lodash": "4.17.21",
    "react": "18.3.1",
    "react-cool-img": "1.2.12",
    "react-day-picker": "9.3.2",
    "react-dom": "18.3.1",
    "react-hotkeys": "2.0.0",
    "react-markdown": "9.0.1",
    "react-papaparse": "4.4.0",
    "react-placeholder": "4.1.0",
    "react-popper": "2.3.0",
    "react-router-dom": "6.28.0",
    "react-router-hash-link": "2.4.3",
    "react-select": "5.8.3",
    "react-syntax-highlighter": "15.6.1",
    "react-time-picker": "7.0.0",
    "react-use-websocket": "4.11.1",
    "reactflow": "11.11.4",
    "remark-gfm": "4.0.0",
    "semver": "7.6.3",
    "use-deep-compare-effect": "1.8.1",
    "uuid": "11.0.3",
    "web-vitals": "4.2.4"
